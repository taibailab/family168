CREATE MEMORY TABLE JBPM_JOB(DBID_ BIGINT GENERATED BY DEFAULT AS IDENTITY(START WITH 1) NOT NULL PRIMARY KEY,CLASS_ VARCHAR(255) NOT NULL,DBVERSION_ INTEGER NOT NULL,DUEDATE_ TIMESTAMP,ISSUSPENDED_ BOOLEAN,ISEXCLUSIVE_ BOOLEAN,LOCKOWNER_ VARCHAR(255),LOCKEXPTIME_ TIMESTAMP,INFO_ VARCHAR(255),EXCEPTION_ LONGVARCHAR,RETRIES_ INTEGER,PROCESSINSTANCE_ BIGINT,EXECUTION_ BIGINT,SIGNAL_ VARCHAR(255),EVENT_ VARCHAR(255),REPEAT_ VARCHAR(255),CONSTRAINT FK_JOB_PRINST FOREIGN KEY(PROCESSINSTANCE_) REFERENCES JBPM_EXECUTION(DBID_),CONSTRAINT FK_TMR_EXECUTION FOREIGN KEY(EXECUTION_) REFERENCES JBPM_EXECUTION(DBID_))
CREATE MEMORY TABLE JBPM_LOB(DBID_ BIGINT GENERATED BY DEFAULT AS IDENTITY(START WITH 1) NOT NULL PRIMARY KEY,DBVERSION_ INTEGER NOT NULL,BLOB_VALUE_ LONGVARBINARY,BINARY_VALUE_ VARBINARY(255),CLOB_VALUE_ LONGVARCHAR,TEXT_VALUE_ LONGVARCHAR,DEPLOYMENT_ BIGINT,NAME_ LONGVARCHAR,CONSTRAINT FK_LOB_DEPLOYMENT FOREIGN KEY(DEPLOYMENT_) REFERENCES JBPM_DEPLOYMENT(DBID_))
CREATE MEMORY TABLE JBPM_PARTICIPATION(DBID_ BIGINT GENERATED BY DEFAULT AS IDENTITY(START WITH 1) NOT NULL PRIMARY KEY,DBVERSION_ INTEGER NOT NULL,GROUPID_ VARCHAR(255),USERID_ VARCHAR(255),TYPE_ VARCHAR(255),TASK_ BIGINT,SWIMLANE_ BIGINT)
